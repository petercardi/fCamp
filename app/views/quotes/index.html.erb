<div class="container">

  <div class="page-header">
    <div class="pull-right">
      <%= link_to "Add Quote", new_quote_path, class: "btn btn-primary" %>
    </div>
    <h1>Quotes</h1>
  </div>

  <%= @quotes.each do |quote| %>
    <div class="row">
      <div class="col-md-12">
        <%= quote.text %>
        <%= quote.author %>
      </div>
    </div>
  <% end %>


</div
